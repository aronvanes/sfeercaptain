extends layout

block content
  header
    h1.pagename Registreren

  form.register(action='/register',method='post')
    div#naam.input
      label(for='naam') Naam:
      input(type='text', name='naam')
    div#email.input
      label(for='email') Email:
      input(type='email', name='email')
    div#age.input
      label(for='age') Leeftijd:
      input(type='number', name='age')
    div#seizoen.input
      p Hebt u een seizoenspas?
      label(for= 'seizoen') ja
      input(type='radio' name='seizoen')
      label(for= 'seizoen') nee
      input(type='radio' name='seizoen')
    button.submit(type='submit') Registreer


